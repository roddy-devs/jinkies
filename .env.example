# Django Webhook (Optional - for Django integration)
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/YOUR_WEBHOOK_ID/YOUR_WEBHOOK_TOKEN

# Environment
ENVIRONMENT_NAME=production
LOG_LEVEL=INFO
